  "scripts": {
    "storybook": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 9009 -s public",
    "build-storybook": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=4096' build-storybook -s public --quiet",
